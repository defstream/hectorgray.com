{
  "name": "just-copy-hectorgray.com",
  "author": "Hector Gray <hgray@incredi.co> (http://github.com/defstream)",
  "license": "MIT",
  "version": "1.0.1",
  "description": "www.hectorgray.com",
  "directories": {
    "doc": "doc",
    "test": "test",
    "src": "src"
  },
  "repository": "https://github.com/defstream/just-copy-hectorgray.com.git",
  "scripts": {
    "doc": "jsdoc -R ./README.md -r -d ./doc/api ./src/js/app.js",
    "test": "snyk test && istanbul cover --dir ./doc/coverage _mocha -- --recursive",
    "analyze": "plato -d ./doc/analysis -r ./lib ./src/js/main.njsx",
    "dev": "webpack-dev-server -d --progress --colors --port 8090 --watch",
    "build": "webpack --optimize-minimize --optimize-occurence-order --optimize-dedupe && grunt build",
    "start": "npm run serve",
    "serve": "npm run build && ./node_modules/.bin/http-server -p 8080 ./build"
  },
  "keywords": [
    "just",
    "copy",
    "hector",
    "gray",
    "hectorgray.com"
  ],
  "dependencies": {
    "babel": "^6.1.18",
    "react": "^0.14.5",
    "react-dom": "^0.14.5",
    "regenerator": "0.8.42",
    "regenerator-loader": "2.0.0",
    "superagent": "1.6.1"
  },
