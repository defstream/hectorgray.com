    "eslint-plugin-react": "^3.13.1",
    "express-session": "^1.11.2",
    "file-loader": "^0.8.5",
    "gifsicle": "^2.0.0",
    "grunt": "~0.4.1",
    "grunt-autoprefixer": "^1.0.0",
